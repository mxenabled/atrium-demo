<h2>Members New</h2>
<%= @code %>
<div>
    <%=form_for :user, class: "member_credentials" do |f| %>
        <% for @credential in @credentials %>
        <%= f.label @credential.label %>
        <%= f.text_field @credential.guid %>
        <% end %>
        <%= f.submit "Create Member" %>
    <% end %>
</div>
