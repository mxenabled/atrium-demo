<div>
<%= @member_guid %>
<%=form_with url: '/resume_agg' do |f| %>
    <% for @challenge in @challenges %>
        <% @challenge_info = str_to_hash(@challenge) %>
        <%= f.label @challenge_info[:guid] %>
        <%= f.label @challenge_info[:label] %>
        <%= f.text_field "challenges_arr[#{@challenge_info[:guid]}]" %>
        <%= f.hidden_field 'member_guid', :value => @member_guid %>
        <% end %>
    <%= f.submit "Submit" %>
<% end %>
</div>