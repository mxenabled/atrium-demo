  <tr class="institution">
    <td>
        <img class="institution_image" src='<%=institution.small_logo_url %>'>
        <div class="button is-dark institution-button">
        <%= link_to "#{institution.name}",{
          :controller => "members",
          :action => "new",
          :method => "get",
          :institution_code => institution.code
        }, class: "institution-link" %>
      </div>
    </td>
  </tr>